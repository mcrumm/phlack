{
    "name": "Slack API",
    "apiVersion": "v1",
    "description": "Slack API",
    "baseUrl": "https://slack.com",
    "operations": {
        "ListChannels": {
            "httpMethod": "GET",
            "uri": "/api/channels.list",
            "summary": "Get the list of channels",
            "responseClass":"ListChannelsOutput",
            "parameters": {
                "token": {
                    "location": "query",
                    "type": "string",
                    "required": true
                },
                "pretty": {
                    "location": "query",
                    "type":     "integer",
                    "required": false,
                    "default": 0
                }
            },
            "additionalParameters": {
                "location": "query"
            }
        },
        "ListFiles": {
            "httpMethod": "GET",
            "uri": "/api/files.list",
            "summary": "Get a list of files",
            "responseClass":"ListFilesOutput",
            "parameters": {
                "token": {
                    "location": "query",
                    "type": "string",
                    "required": true
                },
                "pretty": {
                    "location": "query",
                    "type":     "integer",
                    "required": false,
                    "default": 0
                },
                "user": {
                    "location": "query",
                    "type":     "string",
                    "required": false
                },
                "from": {
                    "location": "query",
                    "sentAs":   "ts_from",
                    "type":     "integer",
                    "required": false
                },
                "to": {
                    "location": "query",
                    "sentAs":   "ts_to",
                    "type":     "integer",
                    "required": false
                },
                "types": {
                    "location": "query",
                    "type":     "string",
                    "required": false,
                    "default":  "all"
                },
                "count": {
                    "location": "query",
                    "type":     "integer",
                    "required": false,
                    "default":  100
                },
                "page": {
                    "location": "query",
                    "type":     "integer",
                    "required": false,
                    "default":  1
                }
            },
            "additionalParameters": {
                "location": "query"
            }
        },
        "ListGroups": {
            "httpMethod": "GET",
            "uri": "/api/groups.list",
            "summary": "Get the list of groups",
            "responseClass":"ListGroupsOutput",
            "parameters": {
                "token": {
                    "location": "query",
                    "type": "string",
                    "required": true
                },
                "pretty": {
                    "location": "query",
                    "type":     "integer",
                    "required": false,
                    "default": 0
                }
            },
            "additionalParameters": {
                "location": "query"
            }
        },
        "ListIM": {
            "httpMethod": "GET",
            "uri": "/api/im.list",
            "summary": "Get the list of instant message channels",
            "responseClass":"ListIMOutput",
            "parameters": {
                "token": {
                    "location": "query",
                    "type": "string",
                    "required": true
                }
            },
            "additionalParameters": {
                "location": "query"
            }
        },
        "ListUsers": {
            "httpMethod": "GET",
            "uri": "/api/users.list",
            "summary": "Get the list of users",
            "responseClass":"ListUsersOutput",
            "parameters": {
                "token": {
                    "location": "query",
                    "type": "string",
                    "required": true
                }
            },
            "additionalParameters": {
                "location": "query"
            }
        },
        "ChannelsJoin": {
            "httpMethod": "GET",
            "uri": "/api/channels.join",
            "summary": "Join a channel. If the channel does not exists, it is created",
            "responseClass":"ChannelsJoinOutput",
            "parameters": {
                "token": {
                    "location": "query",
                    "type": "string",
                    "required": true
                },
                "name": {
                    "location": "query",
                    "type":     "string",
                    "required": true
                }
            }
        },
        "ChannelsSetPurpose": {
            "httpMethod": "GET",
            "uri": "/api/channels.setPurpose",
            "summary": "Change the purpose of a channel",
            "responseClass":"ChannelsSetPurposeOutput",
            "parameters": {
                "token": {
                    "location": "query",
                    "type": "string",
                    "required": true
                },
                "channel": {
                    "location": "query",
                    "type":     "string",
                    "required": true
                },
                "purpose": {
                    "location": "query",
                    "type":     "string",
                      "required": true
                }
            }
        }
    },
